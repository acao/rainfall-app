{
  "extends": "rackt",
  "env": {
    "es6": true
  },
  "rules": {
    "valid-jsdoc": 2,
    "no-var": 0,
    "react/jsx-uses-react": 1,
    "react/jsx-no-undef": 2,
    "react/wrap-multilines": 2
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ]
}
